import java.util.Arrays;
import java.util.LinkedList;

public class SampleLinkedListExample 
{
	public static void main(String[] args) 
	{
		LinkedList a = new LinkedList(Arrays.asList(11,"AkAsh",21,21.21,"Pune","Latur"));
		
		System.out.println(a);	//[11, AkAsh, 21, 21.21, null, true]
		System.out.println(a.size());	//6
		System.out.println(a.get(1));	//AkAsh
		System.out.println(a.isEmpty()); //false
		System.err.println(a.remove(4));	//null
		System.out.println(a);	//[11, AkAsh, 21, 21.21, true]
		System.out.println(a.size());	//5
		System.out.println(a.set(3,"Yadav"));	//Yadav
		System.out.println(a);	//[11, AkAsh, 21, Yadav, true]
		System.out.println(a.size());	//5
		
	}
}
