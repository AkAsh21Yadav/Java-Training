package Lombok_Example;

public class LombokTest 
{
	private int id;
	private String name;
	private String org;
	
	public LombokTest(int id, String name, String org) 
	{
		this.id = id;
		this.name = name;
		this.org = org;
	}
	
	public int getId()
	{
		return id;
	}
	
	public void setId(int id)
	{
		this.id = id;
	}
	public String getName() 
	{
		return name;
	}
	public void setName(String name) 
	{
		this.name = name;
	}
	public String getOrg() 
	{
		return org;
	}
	public void setOrg(String org) 
	{
		this.org = org;
	}
	
	
	
}
