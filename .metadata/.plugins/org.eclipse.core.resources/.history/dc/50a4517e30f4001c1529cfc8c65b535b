import java.util.Arrays;

public class ArrayBasic 
{
	public static void main(String[] args) 
	{
		String[] names = new String[5];
		names[0] = "Virat";
		names[1] = "Rohit";
		names[2] = "Mithali";
		names[3] = "Smriti";
		names[4] = "Dhoni";
		System.out.println("Size of First Array : " +names.length);
		
		System.out.println("\n1st element : " +names[0]);
		System.out.println("2nd element : " +names[1]);
		System.out.println("3rd element : " +names[2]);
		System.out.println("4th element : " +names[3]);
		System.out.println("5th element : " +names[4]);
			
		int[] nums = {12,20,30,40};
		System.out.println("\nSize of Second Array : " +nums.length);
		for (int i : nums) ]
		{
			System.out.println(i);
		}
		
		System.out.println("\nElements of Array : " +Arrays.toString(nums));
	}
}