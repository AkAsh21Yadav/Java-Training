

import java.util.ArrayList;

public class ProductArrayListExample2 
{
	int productId;
	String name;
	
	public ProductArrayListExample2(int productId, String name) 
	{
		this.productId = productId;
		this.name = name;
	}

	@Override
	public String toString() {
		return "ProductArrayListExample [productId=" + productId + ", name=" + name + "]";
	}
	
	public static void main(String[] args) 
	{
		ArrayList p1 = new ArrayList(1,"Mobile");
	}
	
}
