package com.ak.interface_example;

interface Vehicle
{
	abstract void color();
	abstract void price();
	public default void brand()
	{
		System.out.println("Brand : Mersedes");
	}
}

class Car implements Vehicle
{

	@Override
	public void color() 
	{
		
	}

	@Override
	public void price() 
	{
		
	}
	
}

public interface RunnerVehicle 
{

}
