package com.ak.constructorchaining;

public class WithinClass 
{
	WithinClass(int a)
	{
		this(12,12);
		System.out.println(a);
	}
	public WithinClass(int a,int b) 
	{
		this(12,12,12);
		System.out.println(a*b);
	}
	public WithinClass(int a,int b,int c) 
	{
		System.out.println(a+b+c);
	}
	
	public static void main(String[] args) 
	{
		new WithinClass(5);
	}
}
