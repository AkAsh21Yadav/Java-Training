package com.akash.datahiding;

class Animal
{
	private int age;
	private String name;
	
	public int getAge() 
	{
		return age;
	}
	public void setAge(int age) 
	{
		this.age = age;
	}
	public String getName() 
	{
		return name;
	}
	public void setName(String name) 
	{		
		this.name = name;
	}	
}

public class Main 
{
	public static void main(String[] args) 
	{
		Aniaml a = new Aniaml();
		
	}
}
