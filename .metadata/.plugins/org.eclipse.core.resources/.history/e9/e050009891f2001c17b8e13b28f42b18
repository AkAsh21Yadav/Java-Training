package com.ak.constructorchaining;

public class WithinClass 
{
	int a,b;
	
    WithinClass(int a.int b) 
	{
		this();
		System.out.println(a*b);
	}
	WithinClass()
	{
		System.out.println("Default Constructor");
	}
	WithinClass(int a)
	{
		this(10,5);
		System.out.println(a);
	}
	
	public static void main(String[] args) 
	{
		new WithinClass();
	}
	
}
