
public class SwitchEx 
{
	public static void main(String[] args) 
	{
		int x = 2;
		switch(x)
		{
			case 1:
				System.out.println("Ranchi");
				break;
			case 2:
				System.out.println("Pune");
			//	break;
			case 3:
				System.out.println("Raipur");
				break;
			default:
				System.out.println("Kolkata");
				break;
		}
	}
}