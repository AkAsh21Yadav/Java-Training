import java.util.Arrays;
import java.util.TreeSet;

public class SampleTreeSetExample 
{
	public static void main(String[] args) 
	{
		TreeSet<Integer> t = new TreeSet(Arrays.asList(10,20,30,40,50));
		System.out.println(t);
		System.out.println(t.pollFirst());
		System.out.println(t.pollLast());
		System.out.println(t);
		System.out.println(t.first());
		System.out.println(t.last());
		System.out.println(t.higher(20));
		System.out.println(t.lower(30));
		System.out.println(t.ceiling(30));
		System.out.println(t.floor(50));
		
	}
}
