package com.akash.datahiding;

class Calculator
{
	private int a,b,c;
	private String s1,s2;

	public int getAdd()
	{
		return c = a+b;
	}
	
	public void setAdd(int a,int b) 
	{
		this.a = a;
		this.b = b;
	}

	public String getS1() 
	{
		return s1;
	}
	public void setS1(String s1) 
	{
		this.s1 = s1;
	}
	public String getS2() 
	{
		return s2;
	}
	public void setS2(String s2) 
	{
		this.s2 = s2;
	}		
}

public class Main1 
{
	public static void main(String[] args) 
	{
		Calculator c =  new Calculator();
		
	}
}
