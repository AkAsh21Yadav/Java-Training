import java.util.ArrayList;

public class SampleArrayListExampleofTwoCollections 
{
	public static void main(String[] args) 
	{
		ArrayList<Character> char1 = new ArrayList<>();
		char1.add('A');
		char1.add('B');
		char1.add('C');
		char1.add('D');
		System.out.println("1st Collection :"+char1);
		
	}
}
