package com.ak.constructorchaining;

class Within 
{
	Within()
	{
		System.out.println("Default Constructor");
	}
	Within(int a)
	{
		this(10,5);
		System.out.println(a);
	}
	Within(int a,int b)
	{
		this(50);
		System.out.println(a*b);
	}
	public static void main(String[] args) 
	{
		new Within();
	}
	
}
