
class capsul
{
	private int id;	//Declaration
	private String name;
	
	public capsul()  //Initialization
	{
		id = 21;
		name = "AkAsh";
	}
	
	public void display()  //Utilization
	{
		System.out.println("Id : "+id+"\nName : "+name);
	}
}

public class Encapsulation 
{
	public static void main(String[] args) 
	{

		capsul c = new capsul();
		c.display();
	}
	
}
