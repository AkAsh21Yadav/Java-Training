package com.ak.constructorchaining;

class WithinClass 
{
	WithinClass(int a.int b) 
	{
		this(50);
		System.out.println(a*b);
	}
	WithinClass()
	{
		System.out.println("Default Constructor");
	}
	WithinClass(int a)
	{
		this(10,5);
		System.out.println(a);
	}
	
	public static void main(String[] args) 
	{
		new WithinClass();
	}
	
}
