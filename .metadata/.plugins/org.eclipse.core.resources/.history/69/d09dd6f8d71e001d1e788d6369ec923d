import java.util.Scanner;
/*
  			1
Pattern =	22
			333 
 */
public class Number 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("Enter the Number : ");
		int row = s.nextInt();
		
	
		for(int i=0;i<=row;i++)
		{
			for(int j=0;j<i;j++)
			{
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}
}
