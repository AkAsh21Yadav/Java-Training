import java.util.Stack;

public class SampleStack 
{
	public static void main(String[] args) 
	{
		Stack s = new Stack();
		
		s.add(21);
		s.add("AkAsh");
		s.add(true);
		s.add(null);
		s.add(11.11);
		s.add(true);
		
		System.out.println(s);
		System.out.println(s.size());
		System.out.println(s.clone());
	}

}
