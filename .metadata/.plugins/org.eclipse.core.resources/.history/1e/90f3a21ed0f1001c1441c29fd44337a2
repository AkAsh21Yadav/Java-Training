class  Parent
{  
	void printData() 
	{  
		System.out.println("method of parent class");  
	}  
}  
class Child extends Parent 
{  
	void printData() 
	{  
		System.out.println("method of child class");  
	}  
}  
class UpcastingExample
{  
	public static void main(String args[]) 
	{  
		Parent p1 = new Child();  
	//	Parent p2 = new Child();   
		p1.printData();  
	//	p2.PrintData();  
	}  
}  
