package com.akash.datahiding;

class Dog
{
	private int price;
	private String color,name;
	
	int getprice()
	{	return price;	}
	void setPrice(int price)
	{	this.price = price;	}
	String getColor()
	{	return color;	}
	void getColor(String color)
	{	this.color = color;	}
	String getName()
	{	return name;	}
	void setName(String name)
	{	this.name = name;	}
}

public class Animal1 
{
	public static void main(String[] args) 
	{
		Dog d = new Dog();
		d.setInfo(23145,5,"Labrador","Black","Alpha");
		System.out.println(d.getInfo("Name : "+name+"Color : "+color+"Breed : "+breed+"Age : "+age+"Price : "+price));
	}
}
