
public class SwitchCase {
public static void main(String[] args) {
	
	int x = 4;
	switch(x) 
	{
		case 1 :
			System.out.println("USA");
			break;
		case 2:
			System.out.println("Europe");
			break;
		case 3:
			System.out.println("Africa");
			break;
		case 4:
			System.out.println("\t\t\tAsia");
			break;
		default:
			System.out.println("Russia");
	}
}
}