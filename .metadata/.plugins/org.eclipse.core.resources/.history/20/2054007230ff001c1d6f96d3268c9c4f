import java.util.Arrays;
import java.util.LinkedList;

public class SampleLinkedListExample 
{
	public static void main(String[] args) 
	{
		System.out.println("Heterogenous LinkedList : ");
		LinkedList a = new LinkedList(Arrays.asList(11,"AkAsh",21,21.21,null,true));
		
		System.out.println(a);	//[11, AkAsh, 21, 21.21, null, true]
		System.out.println(a.size());	//6
		System.out.println(a.get(1));	//AkAsh
		System.out.println(a.isEmpty()); //false
		System.err.println(a.remove(4));	//null
		System.out.println(a);	//[11, AkAsh, 21, 21.21, true]
		System.out.println(a.size());	//5
		System.out.println(a);	//[11, AkAsh, 21, Yadav, true]
		System.out.println(a.size());	//5
		
		System.out.println("\nHomogenous LinkedList : ");
		LinkedList<Integer> b = new LinkedList<>(Arrays.asList(21,22,23,24,25));

		System.out.println(b);
		System.out.println(b.size());
		System.out.println(b.contains(21));
		System.out.println(b.isEmpty());
		System.out.println(b.remove(3));
		System.out.println(b);
		System.out.println(b.size());
		System.out.println(b.get(3));
		System.out.println(b.set(2,35));
		System.out.println(b);
		System.out.println(b.size());
		b.clear();
		System.out.println(b);
	}
}
